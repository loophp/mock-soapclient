{
    "name": "loophp/mock-soapclient",
    "description": "A fake and predictable SOAP client, useful for testing.",
    "keywords": [
        "testing",
        "soap",
        "ext-soap",
        "mock"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Pol Dellaiera",
            "email": "pol.dellaiera@protonmail.com"
        }
    ],
    "require": {
        "php": ">= 7.4",
        "ext-soap": "*"
    },
    "require-dev": {
        "drupol/php-conventions": "^2.0.2",
        "friends-of-phpspec/phpspec-code-coverage": "^4.3.2",
        "infection/infection": "^0.20.1",
        "infection/phpspec-adapter": "^0.1.1",
        "phpspec/phpspec": "^6.3 || ^7",
        "vimeo/psalm": "^4"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "loophp\\MockSoapClient\\": "./src/"
        }
    }
}
