{
    "name": "loophp/mock-soapclient",
    "description": "A friendly mockable SOAP client, useful for tests.",
    "keywords": [
        "testing",
        "soap",
        "ext-soap",
        "mock"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Pol Dellaiera",
            "email": "pol.dellaiera@protonmail.com"
        }
    ],
    "require": {
        "ext-soap": "*"
    },
    "require-dev": {
        "drupol/php-conventions": "^1.7.1 || ^1.8.4",
        "friends-of-phpspec/phpspec-code-coverage": "^4.3",
        "infection/infection": "^0.17.5",
        "phpspec/phpspec": "^5.1.2 || ^6.2.1",
        "vimeo/psalm": "^3.12.2"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "loophp\\MockSoapClient\\": "./src/"
        }
    }
}
